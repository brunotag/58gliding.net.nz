{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "bogardo/mailgun": "^5.1",
        "coenjacobs/eloquent-composite-primary-keys": "^1.0",
        "doctrine/dbal": "^2.9",
        "felixkiss/uniquewith-validator": "^3.2",
        "fideloper/proxy": "^4.0",
        "graham-campbell/markdown": "^11.0",
        "guzzlehttp/guzzle": "^6.3",
        "infyomlabs/adminlte-templates": "5.8.x-dev",
        "infyomlabs/laravel-generator": "5.8.x-dev",
        "justbetter/laravel-pagination-with-havings": "^1.4",
        "kriswallsmith/buzz": "^1.0",
        "laracasts/presenter": "^0.2.1",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.3",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "5.8.*",
        "leantony/laravel-grid": "2.0.x-dev",
        "maatwebsite/excel": "~3.0",
        "nyholm/psr7": "^1.1",
        "php-http/guzzle6-adapter": "^1.1"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/homestead": "^7.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5",
        "sven/artisan-view": "^3.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
       ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
